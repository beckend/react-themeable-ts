{
  "name": "react-themeable-ts",
  "version": "0.4.1",
  "description": "Handle classname and style props in React",
  "homepage": "https://github.com/beckend/react-themeable-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beckend/react-themeable-ts.git"
  },
  "bugs": {
    "url": "https://github.com/beckend/react-themeable-ts/issues"
  },
  "main": "./build/index.js",
  "jsnext:main": "./build-es2015/index.js",
  "typings": "./build/index.d.ts",
  "scripts": {
    "test:compile": "./node_modules/typescript/bin/tsc --p src/tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepare:publish": "npm run test:compile && gulp",
    "ci": "npm run test:compile && gulp ci"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/ts-jest/preprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(spec))\\.(ts|tsx)$",
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "lcovonly",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "__TS_CONFIG__": "./src/tsconfig.test.json"
    }
  },
  "keywords": [
    "typescript",
    "react",
    "style",
    "classname"
  ],
  "author": "Linken Dinh",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/chai": "^3.4.34",
    "@types/classnames": "0.0.31",
    "@types/debug": "0.0.29",
    "@types/fs-extra": "0.0.33",
    "@types/gulp": "^3.8.32",
    "@types/gulp-debug": "^2.0.29",
    "@types/gulp-typescript": "0.0.32",
    "@types/jest": "^15.1.32",
    "@types/lodash": "^4.14.37",
    "@types/merge2": "^0.3.29",
    "@types/object-path": "^0.9.28",
    "@types/react": "^0.14.39",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "debug": "^2.2.0",
    "enzyme": "^2.4.1",
    "fs-extra": "^0.30.0",
    "globby": "^6.0.0",
    "google-closure-compiler-js": "^20160916.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-debug": "^2.1.2",
    "gulp-run": "^1.7.1",
    "gulp-tslint": "^6.1.2",
    "gulp-typescript": "^3.0.2",
    "merge2": "^1.0.2",
    "react": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "ts-jest": "^0.1.7",
    "ts-node": "^1.4.3",
    "tslint": "^3.15.1",
    "tslint-microsoft-contrib": "^2.0.12",
    "typescript": "^2.0.3"
  },
  "engines": {
    "node": ">=6.7.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash.assign": "^4.2.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.size": "^4.2.0",
    "memoizee": "^0.4.1",
    "object-path": "^0.11.2"
  },
  "peerDependencies": {
    "react": ">=15.0.0"
  }
}
