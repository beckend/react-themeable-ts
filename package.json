{
  "name": "react-themeable-ts",
  "version": "0.4.7",
  "description": "Handle classname and style props in React",
  "homepage": "https://github.com/beckend/react-themeable-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beckend/react-themeable-ts.git"
  },
  "bugs": {
    "url": "https://github.com/beckend/react-themeable-ts/issues"
  },
  "files": [
    "build-es2015",
    "build-es2017"
  ],
  "main": "./build-es2015/index.js",
  "jsnext:main": "./build-es2015/index.js",
  "types": "./build-es2015/index.d.ts",
  "scripts": {
    "prepare": "gulp",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --no-cache",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(spec))\\.(ts|tsx)$",
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "lcovonly",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "__TS_CONFIG__": "./src/tsconfig-test.json"
    }
  },
  "keywords": [
    "typescript",
    "react",
    "style",
    "classname"
  ],
  "author": "Linken Dinh",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/chai": "^3.4.34",
    "@types/classnames": "0.0.32",
    "@types/debug": "0.0.29",
    "@types/fs-extra": "0.0.34",
    "@types/gulp": "^3.8.32",
    "@types/gulp-debug": "^2.0.29",
    "@types/jest": "^16.0.0",
    "@types/lodash": "^4.14.39",
    "@types/object-path": "^0.9.28",
    "@types/react": "^0.14.48",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "cmd-spawn": "^1.2.0",
    "coveralls": "^2.11.15",
    "debug": "^2.4.1",
    "enzyme": "^2.6.0",
    "fs-extra": "^1.0.0",
    "globby": "^6.1.0",
    "google-closure-compiler-js": "^20161024.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-debug": "^3.0.0",
    "gulp-tslint": "^6.1.3",
    "jest": "^17.0.0",
    "react": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "shell-quote": "^1.6.1",
    "ts-jest": "^17.0.0",
    "ts-node": "^1.7.0",
    "tslint": "^3.15.1",
    "tslint-microsoft-contrib": "^2.0.13",
    "typescript": "^2.1.1"
  },
  "engines": {
    "node": ">=6.7.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash.assign": "^4.2.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.size": "^4.2.0",
    "memoizee": "^0.4.1",
    "object-path": "^0.11.2"
  },
  "peerDependencies": {
    "react": ">=15.0.0"
  }
}
